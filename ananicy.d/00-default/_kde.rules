# Group rules file for default KDE Plasma 5 framework background processes
# Baloo is the file indexer/search daemon for KDE Plasma 5 <https://community.kde.org/Baloo>

{ "name": "baloo_file", "type": "BG_CPUIO" }

{ "name": "krunner",      "type": "LowLatency_RT" }
{ "name": "kwin_x11",     "type": "LowLatency_RT" }
{ "name": "plasmashell",  "nice": -1 }

{ "name": "kdeconnectd",  "type": "BG_CPUIO" }
